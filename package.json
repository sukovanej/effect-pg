{
  "name": "effect-pg",
  "version": "0.9.0",
  "description": "node-pg wrapper for effect-ts",
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "exports": {
    ".": {
      "require": "./index.cjs",
      "import": "./index.js",
      "types": "./index.d.ts"
    },
    "./*": {
      "import": {
        "types": "./*.d.ts",
        "default": "./*.js"
      },
      "require": {
        "types": "./*.d.ts",
        "default": "./*.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup && cp package.json dist/ && cp readme.md dist/",
    "watch": "tsc -w -p tsconfig.json",
    "format": "prettier -w src tests examples",
    "lint": "prettier src tests examples --check",
    "test": "vitest run",
    "version": "changeset version"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@effect/data": "^0.18.5",
    "@effect/io": "^0.40.3",
    "@effect/match": "^0.35.2",
    "@effect/schema": "^0.36.1",
    "@effect/stream": "^0.36.1",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/pg": "^8.10.2",
    "@vitest/coverage-v8": "^0.34.4",
    "effect": "2.0.0-next.34",
    "effect-dotenv": "^0.7.0",
    "prettier": "^3.0.3",
    "tsup": "^7.2.0",
    "tsx": "^3.12.10",
    "typescript": "^5.2.2",
    "vitest": "^0.34.4"
  },
  "dependencies": {
    "pg": "^8.11.3",
    "pg-query-stream": "^4.5.3"
  },
  "peerDependencies": {
    "@effect/data": "^0.18.5",
    "@effect/io": "^0.40.3",
    "@effect/match": "^0.35.2",
    "@effect/schema": "^0.36.1",
    "@effect/stream": "^0.36.1"
  }
}
