{
  "name": "effect-pg",
  "version": "0.23.2",
  "description": "node-pg wrapper for effect-ts",
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "exports": {
    ".": {
      "require": "./index.cjs",
      "import": "./index.js",
      "types": "./index.d.ts"
    },
    "./*": {
      "import": {
        "types": "./*.d.ts",
        "default": "./*.js"
      },
      "require": {
        "types": "./*.d.ts",
        "default": "./*.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup && cp package.json dist/ && cp readme.md dist/",
    "watch": "tsc -w -p tsconfig.json",
    "format": "prettier -w src tests examples",
    "lint": "prettier src tests examples --check",
    "test": "vitest run",
    "version": "changeset version"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@effect/schema": "^0.59.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/pg-cursor": "^2.7.2",
    "@vitest/coverage-v8": "^1.1.3",
    "effect": "2.0.3",
    "effect-dotenv": "^0.16.1",
    "prettier": "^3.1.1",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.3"
  },
  "dependencies": {
    "@types/pg": "^8.10.9",
    "pg": "^8.11.3",
    "pg-cursor": "^2.10.3"
  },
  "peerDependencies": {
    "@effect/schema": "^0.59.0",
    "effect": "^2.0.0"
  }
}
